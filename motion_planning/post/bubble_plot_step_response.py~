### u
set terminal epslatex lw 2 size 8cm,7cm "Helvetica,8pt" #color colortext
set output './figures/u_var_inv.tex'
 
set xlabel '\(t\;[ms]\)' #offset 0,1,0
set ylabel '\(u\;[10^{-1}\;mm^{2}]\)' #offset 3,0,0

# set border lw 1
set key t l spacing 1.5

set yrange [0.98:1.5]
set format y "%.1f"
set ytics (1.0,1.1,1.2,1.3,1.4,1.5)

set xrange [0:40]
# set format x "%.0f"
set xtics (0,10,20,30,40) 

set grid ytics, xtics
a0=0.1
plot './data/data_solution/io/as0_1.dat' using ($1*1000):($2*10) w lp lt 8 pt 4 ps 1 pi 20 lw 1 dt 1 title '$u(t)$',\
     './data/data_solution/io/as0_2.dat' using ($1*1000):($2*10) w lp lt 8 pt 6 ps 1 pi 20 lw 1 dt 1 title '$u_{2}(t)$',\
     './data/data_solution/io/as0_3.dat' using ($1*1000):($2*10) w lp lt 8 pt 8 ps 1 pi 20 lw 1 dt 1 title '$u_{3}(t)$',\
     './data/data_solution/io/as0_4.dat' using ($1*1000):($2*10) w lp lt 8 pt 10 ps 1 pi 20 lw 1 dt 1 title '$u_{4}(t)$',\
     './data/data_solution/io/as0_5.dat' using ($1*1000):($2*10) w lp lt 8 pt 12 ps 1 pi 20 lw 1 dt 1 title '$u_{1/2}(t)$',\
